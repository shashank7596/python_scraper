#!/root/com.rxcorp.cii_python_lib/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'jsonschema','console_scripts','jsonschema'
__requires__ = 'jsonschema'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('jsonschema', 'console_scripts', 'jsonschema')()
    )
